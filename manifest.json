{
    "manifest_version": 2,
    "name": "MAGICAL ANGLES",
    "version": "1.0.0",
    "description": "Rotate the tabs with the help of angles.",
    "icons": {
      "128": "incon128.png",
      "48": "icon48.png",
      "16": "icon16.png"
    },
    "background": {
      "scripts": ["background.js"]
     },

    "browser_action": {
       "default_popup": "popup.html",
       "default_icons": "icon16.png" 
      },
   
    "permissions": ["activeTab"]
  
  }